{
    "name": "iauu-mail",
    "version": "1.0.0",
    "private": true,
    "watch": {
        "server": "src/app.js"
    },
    "scripts": {
        "server": "node src/app",
        "start": "node src/app",
        "dev": "nodemon -L src/app.js",
        "debug": "node --inspect  src/app.js",
        "stop": "pkill --signal SIGINT reforma.ai.server",
        "migrate": "./node_modules/.bin/migrate -c src",
        "test": "mocha --timeout 50000 --exit",
        "test-file": "mocha --timeout 50000 --exit --grep"
    },
    "dependencies": {
        "iauu": "file:/var/lib/iauu",
        "app-module-path": "^2.2.0",
        "aws-sdk": "2.750.0",
        "dotenv": "8.2.0",
        "lodash": "4.17.19",
        "minimist": "1.2.6",
        "moment": "2.27.0",
        "mustache": "4.0.1",
        "nodemailer": "6.4.11",
        "nodemailer-ses-transport": "1.5.1"
    },
    "devDependencies": {
        "chai": "4.2.0",
        "faker": "5.4.0",
        "mocha": "8.1.1",
        "nodemon": "2.0.7",
        "npm-watch": "0.7.0"
    },
    "jest": {
        "testEnvironment": "node"
    }
}
