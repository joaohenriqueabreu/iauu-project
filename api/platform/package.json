{
    "name": "server",
    "version": "0.1.0",
    "private": true,
    "watch": {
        "server": "src/app.js"
    },
    "scripts": {
        "server": "node src/app",
        "start": "node src/app",
        "dev": "nodemon -L src/app.js",
        "debug": "node --inspect  src/app.js",
        "stop": "pkill --signal SIGINT reforma.ai.server",
        "migrate": "./node_modules/.bin/migrate -c src",
        "test": "mocha --timeout 50000 --exit",
        "test-file": "mocha --timeout 50000 --exit --grep",
        "postinstall": "link-module-alias"
    },
    "dependencies": {
        "@hapi/joi": "17.1.1",
        "app-module-path": "^2.2.0",
        "aws-sdk": "2.750.0",
        "axios": "0.21.1",
        "bcryptjs": "2.4.3",
        "body-parser": "1.19.0",
        "compression": "1.7.4",
        "connect-busboy": "0.0.2",
        "cors": "2.8.5",
        "cors-anywhere": "0.4.3",
        "dotenv": "8.2.0",
        "express": "4.17.1",
        "express-fileupload": "1.2.0",
        "express-jwt": "6.0.0",
        "helmet": "3.23.3",
        "jsonwebtoken": "8.5.1",
        "jwt-simple": "0.5.6",
        "lodash": "4.17.19",
        "memcached": "2.2.2",
        "migrate-mongoose": "4.0.0",
        "minimist": "0.2.1",
        "moment": "2.27.0",
        "mongodb": "3.6.0",
        "mongoose": "5.9.27",
        "mongoose-extend-schema": "1.0.0",
        "mustache": "4.0.1",
        "node-cache": "5.1.2",
        "nodemailer": "6.4.11",
        "nodemailer-ses-transport": "1.5.1",
        "pagarme": "^4.15.3",
        "socket.io": "^3.1.2",
        "socketio-jwt": "4.6.2",
        "uid": "1.0.0",
        "uuid": "8.3.0"
    },
    "devDependencies": {
        "chai": "4.2.0",
        "eslint": "7.6.0",
        "eslint-config-strongloop": "2.1.0",
        "faker": "5.4.0",
        "link-module-alias": "^1.2.0",
        "mocha": "8.1.1",
        "nodemon": "2.0.7",
        "npm-watch": "0.7.0",
        "sinon": "9.0.2",
        "sinon-chai": "^3.5.0"
    },
    "jest": {
        "testEnvironment": "node"
    },
    "_moduleAliases": {
        "api": "../platform/src/",
        "src": "../platform/src/",
        "lib": "../lib/"
    }
}
