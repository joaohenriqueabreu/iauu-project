{
    "name": "iauu-notifications",
    "version": "0.1.0",
    "private": true,
    "watch": {
        "server": "src/app.js"
    },
    "scripts": {
        "server": "node src/app",
        "start": "node src/app",
        "dev": "nodemon -L src/app.js",
        "debug": "node --inspect  src/app.js",
        "stop": "pkill --signal SIGINT reforma.ai.server",
        "migrate": "./node_modules/.bin/migrate -c src",
        "test": "mocha --timeout 50000 --exit",
        "test-file": "mocha --timeout 50000 --exit --grep"
    },
    "dependencies": {
        "@iauu/env": "1.0.x",
        "lodash": "4.17.21",
        "moment": "2.27.0",
        "mongodb": "3.6.0",
        "mongoose": "5.9.27",
        "mongoose-extend-schema": "1.0.0",
        "socket.io": "^3.1.2",
        "socketio-jwt": "4.6.2",
        "uid": "1.0.0",
        "uuid": "8.3.0"
    },
    "devDependencies": {
        "nodemon": "^2.0.7",
        "npm-watch": "0.7.0"
    },
    "peerDependencies": {
        "debug": "4.3.1",
        "fsevents": "2.3.2"
    },
    "jest": {
        "testEnvironment": "node"
    }
}
