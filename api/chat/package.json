{
    "name": "iauu-chat",
    "version": "0.1.0",
    "private": true,
    "watch": {
        "server": "src/app.js"
    },
    "scripts": {
        "server": "node src/app",
        "start": "node src/app",
        "dev": "nodemon -L src/app.js",
        "debug": "node --inspect  src/app.js",
        "stop": "pkill --signal SIGINT iauu-chat",
        "migrate": "./node_modules/.bin/migrate -c src",
        "test": "mocha --timeout 50000 --exit",
        "test-file": "mocha --timeout 50000 --exit --grep"
    },
    "dependencies": {
        "iauu": "file:/var/lib/iauu",
        "app-module-path": "^2.2.0",
        "dotenv": "8.2.0",
        "express": "^4.17.1",
        "lodash": "4.17.19",
        "moment": "2.29.4",
        "mongodb": "3.6.0",
        "mongoose": "^5.12.8",
        "mongoose-extend-schema": "1.0.0",
        "socket.io": "^3.1.2",
        "socketio-jwt": "4.6.2",
        "uid": "1.0.0",
        "uuid": "8.3.0",
        "debug": "4.3.1"
    },
    "devDependencies": {
        "nodemon": "^2.0.7",
        "npm-watch": "0.7.0"
    },
    "peerDependencies": {
        "debug": "4.3.1",
        "fsevents": "2.3.2"
    },
    "jest": {
        "testEnvironment": "node"
    }
}
